            "Deligoez\\TCKimlikNo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deligoez\\TCKimlikNo\\TCKimlikNoServiceProvider"
            ],
            "aliases": {
                "TCKimlikNo": "Deligoez\\TCKimlikNo\\Facades\\TCKimlikNo"
